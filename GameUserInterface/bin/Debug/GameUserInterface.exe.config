<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="OpenTK" publicKeyToken="bad199fe84eb3df4" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.1.0.0" newVersion="3.1.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_IConnectService" />
        <binding name="BasicHttpBinding_IConnectService1" />
      </basicHttpBinding>
      <netTcpBinding>
        <binding name="NetTcpBinding_IConnectService">
          <security mode="None">
            <transport clientCredentialType="None" />
            <message clientCredentialType="None" />
          </security>
        </binding>
        <binding name="NetTcpBinding_IConnectService1">
          <security mode="None" />
        </binding>
      </netTcpBinding>
    </bindings>
    <client>
      <endpoint address="net.tcp://localhost:4748/connect" binding="netTcpBinding"
        bindingConfiguration="NetTcpBinding_IConnectService1" contract="ConnectServiceReference.IConnectService"
        name="NetTcpBinding_IConnectService" />
      <endpoint address="http://localhost:4746/connect" binding="basicHttpBinding"
        bindingConfiguration="BasicHttpBinding_IConnectService1" contract="ConnectServiceReference.IConnectService"
        name="BasicHttpBinding_IConnectService" />
    </client>
    <behaviors>
      <serviceBehaviors>
        <behavior name="">
          <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true" />
          <serviceDebug includeExceptionDetailInFaults="false" />
        </behavior>
        <behavior name="CustomBehavior">
          <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <services>
      <service behaviorConfiguration="CustomBehavior" name="WcfServiceLibrary.ConnectService">
        <clear />
        <endpoint address="net.tcp://localhost:4748/connect" binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IConnectService1" contract="WcfServiceLibrary.IConnectService" listenUriMode="Explicit" />
        <endpoint address="" binding="basicHttpBinding" bindingConfiguration="" contract="WcfServiceLibrary.IConnectService" />
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:4746/connect" />
          </baseAddresses>
        </host>
      </service>
    </services>
  </system.serviceModel>
</configuration>